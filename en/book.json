{
    "title": "Creating the New System of Law of the Future: A Global System of Law",
    "description": "An exploration of how Artificial Super Intelligence can enable more dynamic, humane, and effective legal systems for our interconnected world",
    "author": "Alejandro Toledo",
    "language": "en",
    "gitbook": ">=3.0.0",
    "root": ".",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md",
        "languages": "LANGS.md"
    },
    "plugins": [
        "katex",
        "mermaid-gb3",
        "search-pro",
        "back-to-top-button",
        "expandable-chapters",
        "ga",
        "theme-comscore"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "ASI",
                "AI",
                "API",
                "JSON",
                "XML",
                "HTTP"
            ]
        },
        "mermaid-gb3": {
            "theme": "default"
        }
    },
    "pdf": {
        "pageNumbers": true,
        "fontSize": 12,
        "fontFamily": "Arial",
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 56,
            "bottom": 56
        }
    }
}